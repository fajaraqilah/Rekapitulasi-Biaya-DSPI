{
  "rewrites": [
    {
      "source": "/login.html",
      "destination": "/login.html"
    },
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/(.+\\.html|.+\\..+)$",
      "destination": "/$1"
    },
    {
      "source": "/((?!.*\\.).*$)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite"
}